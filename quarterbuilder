#!/usr/bin/env python3

from quarters.builder.jobmanager import JobOverlord, JobDescription

if __name__ == '__main__':
    jo = JobOverlord( 5 )
    jo.start()
    for i in range( 100 ):
        # job description: ujid, pkgname, pkgsrc
        jo.add_job( JobDescription( str( i ), str( i ), 'https://aur.archlinux.org/packages/rr62x/rr62x.tar.gz' ) )
